{
  "name": "v-dropdown",
  "description": "A dropdown layer container component for vue3",
  "version": "3.0.0",
  "author": "TerryZ <terry5@foxmail.com>",
  "type": "module",
  "files": [
    "./dist",
    "./types"
  ],
  "main": "./dist/v-dropdown.umd.cjs",
  "module": "./dist/v-dropdown.js",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/v-dropdown.js"
      },
      "require": "./dist/v-dropdown.umd.cjs"
    }
  },
  "typings": "./types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryZ/v-dropdown.git"
  },
  "keywords": [
    "javascript",
    "vue",
    "web",
    "front-end",
    "dropdown",
    "popup",
    "container"
  ],
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "bootstrap": "^5.3.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "sass": "^1.85.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vitest": "^3.0.6",
    "resize-observer-polyfill": "^1.5.1"
  }
}
