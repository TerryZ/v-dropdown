{
  "name": "v-dropdown",
  "description": "A dropdown layer container plugin for vue3",
  "version": "3.0.0-beta.1",
  "author": "TerryZ <terry5@foxmail.com>",
  "type": "module",
  "files": [
    "/dist",
    "/types"
  ],
  "main": "./dist/v-dropdown.umd.cjs",
  "module": "./dist/v-dropdown.js",
  "exports": {
    ".": {
      "import": "./dist/v-dropdown.js",
      "require": "./dist/v-dropdown.umd.cjs"
    }
  },
  "typings": "types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryZ/v-dropdown.git"
  },
  "keywords": [
    "javascript",
    "vue",
    "web",
    "front-end",
    "dropdown",
    "popup",
    "container"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-c8": "^0.28.4",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.4.1",
    "bootstrap": "^5.3.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "sass": "^1.67.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vitest": "^0.34.4"
  }
}
