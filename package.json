{
  "name": "v-dropdown",
  "description": "A dropdown layer container plugin for vue3",
  "version": "3.0.0-beta.1",
  "author": "TerryZ <terry5@foxmail.com>",
  "type": "module",
  "files": [
    "/dist",
    "/types"
  ],
  "main": "./dist/v-dropdown.umd.cjs",
  "module": "./dist/v-dropdown.js",
  "exports": {
    ".": {
      "import": "./dist/v-dropdown.js",
      "require": "./dist/v-dropdown.umd.cjs"
    }
  },
  "typings": "types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryZ/v-dropdown.git"
  },
  "keywords": [
    "javascript",
    "vue",
    "web",
    "front-end",
    "dropdown",
    "popup",
    "container"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.1.0",
    "bootstrap": "^5.2.2",
    "c8": "^7.12.0",
    "eslint": "^8.25.0",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-css-injected-by-js": "^2.1.0",
    "vitest": "^0.24.0"
  }
}
